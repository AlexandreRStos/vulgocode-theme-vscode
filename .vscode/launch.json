{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceRoot}"],
      "stopOnEntry": false,
      "sourceMaps": true
    },
    {
      "name": "Launch Tests",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceRoot}",
        "--extensionTestsPath=${workspaceRoot}/out/test"
      ],
      "stopOnEntry": false,
      "sourceMaps": true
    }
  ]
}
